{
  "name": "math-chat-app",
  "version": "1.0.0",
  "description": "Interactive math learning chat application with image analysis and question generation",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^8.0.7",
    "katex": "^0.16.8",
    "react-katex": "^3.0.1",
    "lucide-react": "^0.263.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "node-fetch": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
